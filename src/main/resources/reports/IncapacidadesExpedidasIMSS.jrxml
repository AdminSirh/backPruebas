	<parameter name="periodo" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="motivo_incapacidad" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="fecha_inicial_liberacion" class="java.lang.String"/>
	<parameter name="fecha_final_liberacion" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[WITH periodos AS (
    SELECT
        id_nomina,
        periodo,
        fecha_inicial AS periodo_inicio,
        fecha_final AS periodo_fin
    FROM periodos_pago
    WHERE id_nomina = $P{id_nomina}
    AND especial = 0
)
SELECT
    periodos.periodo,
    CAST(
        SUM(
            EXTRACT(DAY FROM LEAST(incapacidades.fecha_final, periodos.periodo_fin) -
                GREATEST(incapacidades.fecha_inicial, periodos.periodo_inicio)) + 1
        ) AS INTEGER
    ) AS conteoDiasIncapacidadPeriodos,
    trabajador_plaza.areas_id,
    trabajador_plaza.tipo_nomina_id,
    catalogo_tipo_nomina.desc_nomina,
    catalogo_ubicacion.id_ubicacion,
    catalogo_ubicacion.desc_ubicacion,
    incapacidades.folio,
    catalogo_motivo_incapacidad.id_motivo_incapacidad,
    catalogo_motivo_incapacidad.inicial_motivo_incapacidad,
    catalogo_tipo_incapacidad.inicial_tipo_incapacidad,
    incapacidades.dias_incapacidad,
   DATE( incapacidades.fecha_inicial) AS fecha_inicial ,
   DATE (incapacidades.fecha_final) AS fecha_final,
   DATE (incapacidades.fecha_movimiento) AS fecha_liberacion,
    incapacidades.expediente,
    CONCAT(persona.apellido_paterno, ' ', persona.apellido_materno, ' ', persona.nombre) AS nombre_completo
FROM
    trabajador_plaza
JOIN catalogo_tipo_nomina ON trabajador_plaza.tipo_nomina_id = id_tipo_nomina
JOIN trabajador ON trabajador_plaza.trabajador_id = id_trabajador
JOIN persona ON trabajador.persona_id = id_persona
JOIN catalogo_ubicacion ON trabajador_plaza.ubicacion_id = id_ubicacion
JOIN catalogo_expedientes ON trabajador.expediente_id = id_expediente
JOIN incapacidades ON CAST(catalogo_expedientes.numero_expediente AS text) = CAST(incapacidades.expediente AS text)
LEFT JOIN catalogo_motivo_incapacidad ON incapacidades.motivo_incapacidad_id = id_motivo_incapacidad
LEFT JOIN catalogo_tipo_incapacidad ON incapacidades.tipo_incapacidad_id = id_tipo_incapacidad
JOIN periodos ON
    incapacidades.fecha_inicial <= periodos.periodo_fin
    AND incapacidades.fecha_final >= periodos.periodo_inicio
WHERE
    periodos.id_nomina = $P{id_nomina}
  AND
  	($P{expediente} IS NULL OR incapacidades.expediente = $P{expediente})
 AND
 	($P{folio} IS NULL OR incapacidades.folio = $P{folio})
AND
    ($P{id_area} IS NULL OR trabajador_plaza.areas_id = $P{id_area})
 AND
   ($P{id_nomina} IS NULL OR trabajador_plaza.tipo_nomina_id = $P{id_nomina})
  AND 
   ($P{motivo_incapacidad} IS NULL OR catalogo_motivo_incapacidad.id_motivo_incapacidad = $P{motivo_incapacidad})
 AND 
   ($P{periodo} IS NULL OR  periodos.periodo = $P{periodo})    
   AND
incapacidades.fecha_movimiento BETWEEN 
    TO_DATE($P{fecha_inicial_liberacion}, 'YYYY-MM-DD') AND 
    TO_DATE($P{fecha_final_liberacion}, 'YYYY-MM-DD')
GROUP BY
    periodos.periodo,
    trabajador_plaza.areas_id,
    trabajador_plaza.tipo_nomina_id,
    catalogo_tipo_nomina.desc_nomina,
    catalogo_ubicacion.id_ubicacion,
    catalogo_ubicacion.desc_ubicacion,
    incapacidades.folio,
    catalogo_motivo_incapacidad.id_motivo_incapacidad,
    catalogo_motivo_incapacidad.inicial_motivo_incapacidad,
    catalogo_tipo_incapacidad.inicial_tipo_incapacidad,
    incapacidades.dias_incapacidad,
    incapacidades.fecha_inicial,
    incapacidades.fecha_final,
    incapacidades.fecha_movimiento,
    incapacidades.expediente,
    CONCAT(persona.apellido_paterno, ' ', persona.apellido_materno, ' ', persona.nombre)
ORDER BY
    periodos.periodo]]>
	</queryString>
