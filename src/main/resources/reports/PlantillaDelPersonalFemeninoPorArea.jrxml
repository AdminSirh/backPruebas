	<parameter name="Orden" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
    desc_area,
    catalogo_expedientes.numero_expediente,
    CONCAT(persona.apellido_paterno, ' ', persona.apellido_materno, ' ', persona.nombre) AS nombre_completo,
    catalogo_puesto.puesto,
    desc_nomina,
    TO_CHAR(fecha_ingreso, 'DD/MM/YYYY') AS fecha_ingreso,
    DATE_PART('year', AGE(CURRENT_DATE, fecha_antiguedad)) AS antiguedad_anios,
    DATE_PART('month', AGE(CURRENT_DATE, fecha_antiguedad)) AS antiguedad_meses,
    DATE_PART('day', AGE(CURRENT_DATE, fecha_antiguedad)) AS antiguedad_dias
FROM trabajador_plaza 
JOIN catalogo_plaza ON catalogo_plaza.id_plaza = trabajador_plaza.plaza_id
JOIN trabajador ON trabajador_plaza.trabajador_id = trabajador.id_trabajador 
JOIN persona ON trabajador.persona_id = persona.id_persona 
JOIN catalogo_expedientes ON trabajador.expediente_id = catalogo_expedientes.id_expediente 
JOIN catalogo_puesto ON catalogo_plaza.puesto_id = catalogo_puesto.id_puesto
JOIN catalogo_areas ON trabajador_plaza.areas_id = catalogo_areas.id_area
JOIN catalogo_tipo_nomina ON trabajador_plaza.tipo_nomina_id = catalogo_tipo_nomina.id_tipo_nomina
WHERE persona.genero_id = 2
ORDER BY desc_area ASC,  $P!{Orden}]]>
	</queryString>
