	<parameter name="id_nomina" class="java.lang.Integer"/>
	<parameter name="id_area" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="periodo" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="motivo_incapacidad" class="java.lang.Integer" isForPrompting="false"/>
	<queryString language="SQL">
		<![CDATA[WITH periodos AS (
    SELECT
        id_nomina,
        periodo,
        fecha_inicial AS periodo_inicio,
        fecha_final AS periodo_fin
    FROM periodos_pago
    WHERE
        ($P{id_nomina} IS NULL OR id_nomina = $P{id_nomina})
        AND especial = 0
)
SELECT DISTINCT
    incapacidades.expediente,
    persona.nombre,
    persona.apellido_paterno,
    persona.apellido_materno,
    persona.num_imss,
    DATE (incapacidades.fecha_inicial) AS fecha_inicial,
    incapacidades.folio,
    incapacidades.dias_incapacidad,
    incapacidades.salario_diario_integrado,
    incapacidades.importe_cobrar_imss
FROM incapacidades
JOIN catalogo_expedientes ON CAST(catalogo_expedientes.numero_expediente AS INTEGER) = incapacidades.expediente
JOIN trabajador ON trabajador.expediente_id = catalogo_expedientes.id_expediente
JOIN persona ON trabajador.persona_id = id_persona
JOIN trabajador_plaza ON trabajador.id_trabajador = trabajador_id
LEFT JOIN catalogo_motivo_incapacidad ON incapacidades.motivo_incapacidad_id = id_motivo_incapacidad
JOIN periodos ON
    incapacidades.fecha_inicial <= periodos.periodo_fin
    AND incapacidades.fecha_final >= periodos.periodo_inicio
WHERE
    --Filtrado de fechas (Obligatorio)
    incapacidades.fecha_inicial >= TO_DATE($P{fecha_inicial}, 'YYYY-MM-DD') 
    AND incapacidades.fecha_final <= TO_DATE($P{fecha_final}, 'YYYY-MM-DD') 
    --Filtrado de expediente (Opcional)
    AND ($P{expediente} IS NULL OR incapacidades.expediente = $P{expediente})
    --Filtrado por folio (Opcional)
    AND ($P{folio} IS NULL OR incapacidades.folio = $P{folio})
    --Filtrado por nómina (Opcional)
    AND ($P{id_nomina} IS NULL OR trabajador_plaza.tipo_nomina_id = $P{id_nomina})
    --Filtrado por área (Opcional)
    AND ($P{id_area} IS NULL OR trabajador_plaza.areas_id = $P{id_area})
    --Filtrado por periodo (Opcional)
    AND ($P{periodo} IS NULL OR periodos.periodo = $P{periodo})    
    --Filtrado por motivo de la incapacidad (Opcional)
    AND ($P{motivo_incapacidad} IS NULL OR catalogo_motivo_incapacidad.id_motivo_incapacidad = $P{motivo_incapacidad})
ORDER BY incapacidades.expediente]]>
	</queryString>
