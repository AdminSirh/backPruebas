	<queryString language="SQL">
		<![CDATA[SELECT
asPersona.nombre,
asPersona.apellido_paterno,
asPersona.apellido_materno, 
asPersona.rfc, 
asPersona.num_imss,
asTrabajador.fecha_ingreso,
asCatalogoExpedientes.numero_expediente,
asCatalogoPuesto.puesto,
asCatalogoAreas.desc_area,
asCatalogoAreaSuperior.desc_area_superior
FROM trabajador_plaza asTrabajadorPlaza
JOIN trabajador asTrabajador ON asTrabajadorPlaza.trabajador_id = id_trabajador
JOIN persona asPersona ON asTrabajador.persona_id = id_persona
JOIN catalogo_expedientes asCatalogoExpedientes ON asTrabajador.expediente_id = id_expediente
JOIN catalogo_plaza asCatalogoPlaza ON asTrabajadorPlaza.plaza_id = id_plaza
JOIN catalogo_puesto asCatalogoPuesto ON asCatalogoPlaza.puesto_id = id_puesto
JOIN catalogo_areas asCatalogoAreas ON asTrabajadorPlaza.areas_id = id_area
JOIN catalogo_area_superior asCatalogoAreaSuperior ON asCatalogoAreas.id_area_superior = asCatalogoAreaSuperior.id_area_superior
WHERE asCatalogoExpedientes.numero_expediente = $P{expediente}]]>
