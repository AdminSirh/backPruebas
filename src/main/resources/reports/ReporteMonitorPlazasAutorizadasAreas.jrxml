	<queryString language="SQL">
		<![CDATA[SELECT catPlaza.numero_plaza,
	trabajadorPlaza.areas_id,
	catPuesto.puesto,
	catPlaza.estatus_plaza_id,
	person.nombre,
	person.apellido_paterno,
	person.apellido_materno,
	catExpediente.numero_expediente
FROM trabajador_plaza trabajadorPlaza
	JOIN trabajador trabajad ON 
	 trabajadorPlaza.trabajador_id = trabajad.id_trabajador 
	JOIN persona person ON 
	 trabajad.persona_id = person.id_persona 
	JOIN catalogo_plaza catPlaza ON 
	 catPlaza.id_plaza = trabajadorPlaza.plaza_id 
	JOIN catalogo_puesto catPuesto ON 
	 catPlaza.puesto_id = catPuesto.id_puesto 
	JOIN catalogo_expedientes catExpediente ON 
	 trabajad.expediente_id = catExpediente.id_expediente 
WHERE 
	 trabajadorPlaza.areas_id = $P{areas_id}]]>
