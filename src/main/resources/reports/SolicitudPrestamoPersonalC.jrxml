	<queryString>
		<![CDATA[SELECT CONCAT ('Lic.', pe.nombre, ' ' ,pe.apellido_paterno, ' ', pe.apellido_materno) AS titular_sub_nomina
FROM catalogo_areas c_a
JOIN catalogo_expedientes c_e ON c_e.id_expediente = c_a.expediente_titular_id
JOIN trabajador tr ON tr.expediente_id = c_e.id_expediente
JOIN persona pe ON pe.id_persona = tr.persona_id
WHERE c_a.expediente_titular_id = 15]]>
	</queryString>
