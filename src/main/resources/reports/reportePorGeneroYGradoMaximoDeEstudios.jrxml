	<queryString language="SQL">
		<![CDATA[SELECT
  cat_estudios.desc_grado,
  cat_genero.desc_genero,
  cat_tipo_nomina.desc_nomina,
  COUNT(*) AS count
FROM
  trabajador_plaza trabajadorPlaza
JOIN catalogo_tipo_nomina cat_tipo_nomina ON trabajadorPlaza.tipo_nomina_id = cat_tipo_nomina.id_tipo_nomina
JOIN trabajador trabajador ON trabajadorPlaza.trabajador_id = trabajador.id_trabajador
JOIN persona persona ON trabajador.persona_id = persona.id_persona
JOIN catalogo_estudios cat_estudios ON persona.grado_estudios_id = cat_estudios.id_grado
JOIN catalogo_genero cat_genero ON persona.genero_id = cat_genero.id_genero
GROUP BY
  cat_estudios.desc_grado,
  cat_genero.desc_genero,
  cat_tipo_nomina.desc_nomina]]>
	</queryString>
