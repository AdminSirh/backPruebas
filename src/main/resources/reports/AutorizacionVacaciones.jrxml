SELECT
    asVacaciones.incidencias_id,
    asVacaciones.expediente,
    asVacaciones.periodo_vacacional,
    asPersona.apellido_paterno,
    asPersona.apellido_materno,
    asPersona.nombre,
    asTrabajador.fecha_ingreso,
    asCatalogoAreaSuperior.desc_area_superior,
    asCatalogoAreas.desc_area,
    asCatalogoPuesto.puesto,
    (asVacaciones.dias_totales - asVacaciones.dias_paseos) AS dias_vacaciones_semana,
    asVacaciones.dias_paseos,
    asVacaciones.dias_totales,
    asVacaciones.fecha_inicio_anio_periodo_vacacional,
    asVacaciones.fecha_fin_anio_periodo_vacacional,
    asVacaciones.fecha_inicio,
    asVacaciones.fecha_fin,
    asVacaciones.observaciones,
    inc.corrida_vacaciones,
    asCatalogoDias.id_dia,
    asCatalogoDias.dia,
    asLibroDias.dia_descanso,
    series.dia AS dia_en_serie,
CASE EXTRACT(DOW FROM series.dia)
-- IGUALACION DE DIAS EN BASE DE DATOS CATALOGO DIAS
    WHEN 0 THEN 1
    WHEN 1 THEN 2
    WHEN 2 THEN 3
    WHEN 3 THEN 4
    WHEN 4 THEN 5
    WHEN 5 THEN 6
    WHEN 6 THEN 7
END AS numero_dia

FROM 
    vacaciones asVacaciones
JOIN 
    trabajador asTrabajador ON asVacaciones.trabajador_id = asTrabajador.id_trabajador
JOIN 
    persona asPersona ON asTrabajador.persona_id = asPersona.id_persona
JOIN 
    trabajador_plaza asTrabajadorPlaza ON asTrabajadorPlaza.trabajador_id = asTrabajador.id_trabajador
JOIN 
    catalogo_areas asCatalogoAreas ON asTrabajadorPlaza.areas_id = asCatalogoAreas.id_area
JOIN 
    catalogo_area_superior asCatalogoAreaSuperior ON asCatalogoAreas.id_area_superior = asCatalogoAreaSuperior.id_area_superior
JOIN 
    catalogo_plaza asCatalogoPlaza ON asTrabajadorPlaza.plaza_id = asCatalogoPlaza.id_plaza
JOIN 
    catalogo_puesto asCatalogoPuesto ON asCatalogoPlaza.puesto_id = asCatalogoPuesto.id_puesto
JOIN 
    libro_dias asLibroDias ON asTrabajador.id_trabajador = asLibroDias.trabajador_id
JOIN
    incidencias inc ON inc.id_incidencia = asVacaciones.incidencias_id
JOIN 
    catalogo_dias asCatalogoDias ON asLibroDias.dia_id = asCatalogoDias.id_dia
JOIN
    series ON series.dia BETWEEN asVacaciones.fecha_inicio::date AND asVacaciones.fecha_fin::date
WHERE 
    asVacaciones.incidencias_id =   $P{incidencias_id}
  LIMIT 1]]>
