	<queryString language="SQL">
		<![CDATA[SELECT cat_expediente.numero_expediente,
persona.nombre, 
persona.apellido_paterno, 
persona.apellido_materno,
cat_genero.desc_genero,
cat_puesto.puesto,
cat_areas.desc_area,
trabajador.fecha_antiguedad,
trabajador.fecha_ingreso
FROM trabajador_plaza trabajadorPlaza JOIN trabajador trabajador ON trabajadorPlaza.trabajador_id = trabajador.id_trabajador 
JOIN catalogo_expedientes cat_expediente ON trabajador.expediente_id = cat_expediente.id_expediente
JOIN persona persona ON trabajador.persona_id = persona.id_persona
JOIN catalogo_plaza cat_plaza ON trabajadorPlaza.plaza_id = cat_plaza.id_plaza
JOIN catalogo_puesto cat_puesto ON cat_plaza.puesto_id = cat_puesto.id_puesto
JOIN catalogo_areas cat_areas ON trabajadorPlaza.areas_id = cat_areas.id_area
JOIN catalogo_genero cat_genero ON persona.genero_id = cat_genero.id_genero
WHERE trabajadorPlaza.tipo_nomina_id=4]]>
	</queryString>
