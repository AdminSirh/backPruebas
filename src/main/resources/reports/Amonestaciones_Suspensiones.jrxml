	<queryString>
		<![CDATA[SELECT CONCAT (persona.nombre, ' ' ,persona.apellido_paterno, ' ' ,persona.apellido_materno) AS nombre_completo,
catalogo_puesto.puesto,
catalogo_areas.desc_area,
c_t_n.desc_nomina,
CONCAT ('LIC.', ' ' ,p_c.nombre, ' ' ,p_c.apellido_paterno, ' ' ,p_c.apellido_materno) AS nombre_subgerente
FROM trabajador_plaza 
JOIN trabajador ON trabajador.id_trabajador = trabajador_plaza.trabajador_id
JOIN persona ON persona.id_persona = trabajador.persona_id
JOIN catalogo_plaza ON catalogo_plaza.id_plaza = trabajador_plaza.plaza_id
JOIN catalogo_puesto ON catalogo_puesto.id_puesto = catalogo_plaza.puesto_id
JOIN catalogo_areas ON catalogo_areas.id_area = trabajador_plaza.areas_id
JOIN catalogo_tipo_nomina c_t_n ON c_t_n.id_tipo_nomina = trabajador_plaza.tipo_nomina_id

JOIN catalogo_expedientes ce_ca ON ce_ca.id_expediente = 88
JOIN trabajador tr_c ON tr_c.expediente_id = ce_ca.id_expediente
JOIN persona p_c ON p_c.id_persona = tr_c.persona_id

JOIN catalogo_expedientes ON catalogo_expedientes.id_expediente = trabajador.expediente_id 
 
WHERE catalogo_expedientes.numero_expediente = $P{expediente}]]>
	</queryString>
