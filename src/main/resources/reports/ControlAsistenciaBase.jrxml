	<queryString>
		<![CDATA[WITH MainQuery AS (
    SELECT 
        asTrabajador.id_trabajador,
        asPeriodosPago.fecha_inicial,
        asPeriodosPago.fecha_final,
        asAreas.desc_area,
        asTrabajadorPlaza.ubicacion_id,
        asPeriodosPago.periodo,
        asPersona.nombre,
        asPersona.apellido_paterno,
        asPersona.apellido_materno,
        asExpediente.numero_expediente,
        asTrabajadorPlaza.tipo_nomina_id,
        asPuesto.puesto,
        dias_descanso.dia_descanso,
        dias_habiles.dia_horario
    FROM trabajador_plaza asTrabajadorPlaza
    JOIN Trabajador asTrabajador ON asTrabajadorPlaza.trabajador_id = asTrabajador.id_trabajador 
    JOIN Persona asPersona ON asTrabajador.persona_id = asPersona.id_persona
    JOIN Catalogo_plaza asPlaza ON asTrabajadorPlaza.plaza_id = asPlaza.id_plaza
    JOIN Catalogo_puesto asPuesto ON asPlaza.puesto_id = asPuesto.id_puesto
    JOIN Catalogo_Expedientes asExpediente ON asTrabajador.expediente_id = asExpediente.id_expediente
    JOIN Catalogo_areas asAreas ON asTrabajadorPlaza.areas_id = asAreas.id_area
    JOIN Periodos_pago asPeriodosPago ON asTrabajadorPlaza.tipo_nomina_id = asPeriodosPago.id_nomina

    -- Subconsulta para obtener los dÃ­as de descanso del trabajador
    LEFT JOIN (
        SELECT trabajador_id, STRING_AGG(CONCAT(dia_id, ':', dia_descanso)::text, ',') as dia_descanso
        FROM Libro_dias
        GROUP BY trabajador_id
    ) dias_descanso ON asTrabajador.id_trabajador = dias_descanso.trabajador_id
    
    LEFT JOIN(
    SELECT 
    Plaza.trabajador_id,
    STRING_AGG(
        REGEXP_REPLACE(
            CONCAT(NombreDias.id_dia,' - ', NombreDias.dia, ' - ', LibroDias.horario_entrada, ' - ', LibroDias.horario_salida),
            '^\d+\s-\s',
            ''
        ),
        ', ' ORDER BY NombreDias.id_dia
    ) AS dia_horario,
    Persona.apellido_paterno
FROM libro_dias as LibroDias
JOIN trabajador_plaza as Plaza ON LibroDias.trabajador_id = Plaza.trabajador_id
JOIN catalogo_dias as NombreDias ON LibroDias.dia_id = id_dia
JOIN trabajador as Trabajador ON Plaza.trabajador_id = id_trabajador
JOIN persona as Persona ON trabajador.persona_id = id_persona
WHERE Plaza.tipo_nomina_id = 1 
AND LibroDias.dia_descanso != 1
GROUP BY Plaza.trabajador_id, Persona.apellido_paterno
ORDER BY Persona.apellido_paterno
	    )dias_habiles ON asTrabajador.id_trabajador = dias_habiles.trabajador_id

    WHERE asTrabajadorPlaza.tipo_nomina_id = 1 
        AND asPeriodosPago.id_nomina = 1
        AND asPeriodosPago.periodo = $P{periodo}
        AND ($P{area_id} IS NULL OR asTrabajadorPlaza.areas_id = $P{area_id})
         AND ($P{expediente} IS NULL OR asExpediente.numero_expediente = $P{expediente})
)

SELECT 
    id_trabajador,
    fecha_inicial,
    fecha_final,
    desc_area,
    ubicacion_id,
    periodo,
    nombre,
    apellido_paterno,
    apellido_materno,
    numero_expediente,
    tipo_nomina_id,
    puesto,
    dia_descanso,
    dia_horario,
    desc_deposito,
    dia,
    comentario,
    estatus
FROM (
    SELECT 
        MainQuery.*,
        ROW_NUMBER() OVER (PARTITION BY id_trabajador ORDER BY apellido_paterno, id_trabajador) as row_num,
        asDepositos.desc_deposito,
        asDias.dia,
		Comentario.comentario,
        asLibroDias.estatus,
        asLibroDias.horario_entrada,
        asLibroDias.horario_salida
    FROM MainQuery
    LEFT JOIN Libro_dias asLibroDias ON MainQuery.id_trabajador = asLibroDias.trabajador_id
    LEFT JOIN Catalogo_dias asDias ON asLibroDias.dia_id = asDias.id_dia
    LEFT JOIN Catalogo_depositos asDepositos ON asLibroDias.deposito_id = asDepositos.id_deposito
    JOIN libro_dias_comentario_general_trabajador AS Comentario ON asLibroDias.trabajador_id =  asLibroDias.trabajador_id
) as subquery
WHERE row_num = 1
AND estatus = 1
ORDER BY  numero_expediente]]>
	</queryString>
