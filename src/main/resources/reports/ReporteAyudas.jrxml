	<queryString language="SQL">
		<![CDATA[SELECT 
Expediente.numero_expediente,
Per.nombre AS nombre_trabajador,
Per.apellido_paterno AS apellido_paterno_trabajador,
Per.apellido_materno AS apellido_materno_trabajador,
Ayuda.fecha_emision,
Inci.fecha_inicio,
Inci.fecha_fin,
Inci.num_dias,
Ayuda.monto,
Ayuda.periodo_pago_id,
Ayuda.nombre AS nombre_ayuda,
Ayuda.apellido_paterno AS apellido_paterno_ayuda,
Ayuda.apellido_materno AS apellido_materno_ayuda,
Parentesco.desc_parentesco,
CatInci.inicial_descripcion,
Inci.folio,
Ayuda.fecha_recepcion,
Nomina.desc_nomina
FROM ayuda AS Ayuda
JOIN incidencias AS Inci ON Inci.id_incidencia = Ayuda.incidencia_id
JOIN catalogo_parentesco AS Parentesco ON Ayuda.parentesco_id = Parentesco.id_parentesco
JOIN catalogo_incidencias AS CatInci ON Inci.incidencia_id = CatInci.id_incidencia
JOIN trabajador_plaza AS TrabPlaza ON Ayuda.trabajador_id = TrabPlaza.trabajador_id 
JOIN catalogo_tipo_nomina AS Nomina ON Nomina.id_tipo_nomina = TrabPlaza.tipo_nomina_id
JOIN trabajador AS Trab ON TrabPlaza.trabajador_id = Trab.id_trabajador
JOIN catalogo_expedientes AS Expediente ON Trab.expediente_id = Expediente.id_expediente
JOIN persona AS Per ON Trab.persona_id = Per.id_persona
WHERE ($P{estadoIncidenciaId} IS NULL OR estado_incidencia_id = $P{estadoIncidenciaId})
AND (($P{fechaInicio} IS NULL AND $P{fechaFin} IS NULL) OR (date(Inci.fecha_inicio) BETWEEN date($P{fechaInicio}) AND date($P{fechaFin})))]]>
	</queryString>
