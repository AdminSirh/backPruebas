	<queryString>
		<![CDATA[SELECT 
  persona.nombre, persona.apellido_paterno, persona.apellido_materno,
  CONCAT (persona.nombre, ' ' ,persona.apellido_paterno, ' ', persona.apellido_materno) AS nombre_ciudadano,
  persona.rfc, persona.num_imss,
  catalogo_puesto.puesto, catalogo_puesto.clave_rh, catalogo_puesto.nivel,
  catalogo_areas.desc_area,
  catalogo_area_superior.desc_area_superior,
  ce.id_expediente, 
  catalogo_tipo_tabulador.desc_tipo_tabulador as desc_tabulador,
  CONCAT (p_ce.nombre, ' ', p_ce.apellido_paterno, ' ' ,p_ce.apellido_materno ) AS titular_as,
  CONCAT (p_c.nombre , ' ', p_c.apellido_paterno, ' ', p_c.apellido_materno) AS titular_a,
  htp.trabajador_id,
  cpu.puesto as puesto_actual , cpu.clave_rh as clave_actual , cpu.nivel as nivel_actual,
  ca.desc_area as area_actual, cas.desc_area_superior as area_s_actual,
  tp.fecha_captura as fecha_inicio, htp.observaciones as observaciones_actual,
  (
    SELECT 
      CONCAT(persona.nombre, ' ', persona.apellido_paterno, ' ', persona.apellido_materno)
    FROM 
      catalogo_area_superior
      FULL JOIN trabajador ON trabajador.expediente_id = catalogo_area_superior.expediente_titular_id
      FULL JOIN persona ON persona.id_persona = trabajador.persona_id
      FULL JOIN catalogo_expedientes ON catalogo_expedientes.id_expediente = trabajador.expediente_id
    WHERE 
      catalogo_expedientes.id_expediente = 2
  ) AS gerente_capital_humano, 
  
  (
    SELECT 
      CONCAT(persona.nombre, ' ', persona.apellido_paterno, ' ', persona.apellido_materno)
    FROM 
      catalogo_area_superior
      FULL JOIN trabajador ON trabajador.expediente_id = catalogo_area_superior.expediente_titular_id
      FULL JOIN persona ON persona.id_persona = trabajador.persona_id
      FULL JOIN catalogo_expedientes ON catalogo_expedientes.id_expediente = trabajador.expediente_id
    WHERE 
      catalogo_expedientes.id_expediente = 1
  ) AS gerente_finanzas 
FROM 
  trabajador_plaza tp
  FULL JOIN catalogo_tipo_nomina ON catalogo_tipo_nomina.id_tipo_nomina = tp.tipo_nomina_id
  FULL JOIN catalogo_tipo_tabulador ON catalogo_tipo_tabulador.id_tipo_tabulador = catalogo_tipo_nomina.tipo_tabulador_id
  FULL JOIN trabajador ON trabajador.id_trabajador = tp.trabajador_id
  FULL JOIN persona ON persona.id_persona = trabajador.persona_id
  FULL JOIN catalogo_plaza ON catalogo_plaza.id_plaza = tp.plaza_id
  FULL JOIN catalogo_puesto ON catalogo_puesto.id_puesto = catalogo_plaza.puesto_id
  FULL JOIN catalogo_areas ON catalogo_areas.id_area = tp.areas_id
  FULL JOIN catalogo_area_superior ON catalogo_area_superior.id_area_superior = catalogo_areas.id_area_superior
  FULL JOIN catalogo_expedientes ce ON ce.id_expediente = catalogo_area_superior.expediente_titular_id
  FULL JOIN trabajador tr_ce ON tr_ce.expediente_id = ce.id_expediente
  FULL JOIN persona p_ce ON p_ce.id_persona = tr_ce.persona_id
  FULL JOIN catalogo_expedientes ce_ca ON ce_ca.id_expediente = catalogo_areas.expediente_titular_id
  FULL JOIN trabajador tr_c ON tr_c.expediente_id = ce_ca.id_expediente
  FULL JOIN persona p_c ON p_c.id_persona = tr_c.persona_id
  FULL JOIN catalogo_expedientes ON catalogo_expedientes.id_expediente = trabajador.expediente_id 
  FULL JOIN historico_trabajador_plaza htp ON htp.trabajador_plaza_id = tp.id_trabajador_plaza
  FULL JOIN catalogo_plaza cp ON cp.id_plaza = htp.plaza_id
  FULL JOIN catalogo_puesto cpu ON cpu.id_puesto = cp.puesto_id
  FULL JOIN catalogo_areas ca ON ca.id_area = htp.areas_id
  FULL JOIN catalogo_area_superior cas ON cas.id_area_superior = ca.id_area_superior
WHERE 
  catalogo_expedientes.numero_expediente = $P{numero_expediente}
ORDER BY 
  htp.fecha_registro DESC
LIMIT 1]]>
	</queryString>
